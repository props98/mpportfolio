{
  "name": "mpportfolio",
  "version": "1.0.0",
  "description": "Portfolio 2021",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/props98/mpportfolio.git"
  },
  "author": "Maxim Park",
  "license": "ISC",
  "homepage": "https://github.com/props98/mpportfolio#readme",
  "devDependencies": {
    "autoprefixer": "9.5.x",
    "browser-sync": "2.26.x",
    "editorconfig": "^0.15.3",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "8.0.x",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "stylelint": "^13.7.2",
    "stylelint-config-htmlacademy": "0.1.x"
